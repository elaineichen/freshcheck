<div class="page-header">
  <h3>Item #<%= @item.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Category</dt>
      <dd><%= @item.category %></dd>

      <dt>Item name</dt>
      <dd><%= @item.item_name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/items" class="btn btn-default">
        Back
      </a>
      <a href="/items/<%= @item.id %>/edit" class="btn btn-default">
        Edit
      </a>
      <a href="/delete_item/<%= @item.id %>" class="btn btn-default" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
